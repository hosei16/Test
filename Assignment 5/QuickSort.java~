class QuickSort
{
  int partition(int arr [],int low,int high)
  {
    int pivot = arr[high];
    int i = (low-1);
    for(j=0;j<high;j++)
    {
    if(arr[j]<=pivot)
    {
    int temp = arr[i];
    arr[i]=arr[j];
    arr[j]=temp;
    
    
    }
    
    int temp = arr[i+1];
    arr[i+1]=arr[high];
    arr[high]=temp;
    
    }
  }
  static void printArray (int arr[])
  {
  int n= arr.length;
  
  for (i=0;i<n;i++)
    System.out.print (arr[i],+",");
  System.out.println("");
  }