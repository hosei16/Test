def MillerRabin(p):
    d = p - 1
    r = 0

    while d % 2 == 0:
        d //= 2
        r += 1
    a = 101
    x = (a ** d) % p
    if x == 1 or x == p - 1:
        return True

    while r > 1:
        x = (x * x) % p
        if x == 1:
            return False
        if x == p - 1:
            return True
        r -= 1
    return False

print(MillerRabin(5780010198172909744446412707877571532513021623497733477883684595588216951906039455834152177206834017456642847))